#include <iostream>
using namespace std;

int main() {
  int i,n,n1,d= 0,m = 0,d1 = 0,m1 = 0;
  cin >> n;
  i = 0;
    do {
      cin >> n1;
      if(n1 < 0) {
        m += n1*-1;
        m1 ++;
        }
      else {
        d += n1;
        d1 ++;
        }
      i++;
    } while(i < n);
  cout << m/m1 << " " << d/d1;
    return 0;
}
